{
  "name": "graphql-julia-codegen",
  "description": "Generate Julia Types from GraphQL operations/schema",
  "version": "0.0.1",
  "author": "Mark Halonen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laurium-labs/graphql-julia-codegen"
  },
  "homepage": "https://github.com/laurium-labs/graphql-julia-codegen",
  "bugs": "https://github.com/laurium-labs/graphql-julia-codegen/issues",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=8",
    "npm": ">=6"
  },
  "dependencies": {
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.17.0",
    "@oclif/errors": "1.3.4",
    "@oclif/plugin-autocomplete": "0.3.0",
    "@oclif/plugin-help": "2.2.3",
    "@oclif/plugin-not-found": "1.2.4",
    "@oclif/plugin-plugins": "1.10.0",
    "@oclif/plugin-warn-if-update-available": "1.7.0",
    "apollo-codegen-core": "0.39.3",
    "common-tags": "^1.5.1",
    "inflected": "^2.0.3",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.4"
  },
  "devDependencies": {
    "@types/inflected": "1.1.29",
    "@types/jest": "24.9.1",
    "graphql": "15.2.0",
    "typescript": "4.2.3"
  },
  "scripts": {
    "test": "jest"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "oclif": {
    "bin": "mynewcli"
  },
  "jest": {
    "preset": "ts-jest",
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/*.(js|ts)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/lib/",
      "<rootDir>/test/fixtures/",
      "<rootDir>/test/test-utils"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.test.json",
        "diagnostics": false
      }
    }
  }
}
